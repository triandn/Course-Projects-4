@model WebRemotePBL4.Models.FileDetail

@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/Content/style/reset.css">
    <link rel="stylesheet" href="~/Content/style/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css">
    <!-- Add the slick-theme.css if you want default styling -->
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <!-- Add the slick-theme.css if you want default styling -->
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css" />
</head>

<body>
    <!-- ----------------------ZOOM IMAGE---------------------- -->
    <div class="zoom">
        <div class="overlay"></div>
        <button class="slick-btn close"><i class="fa fa-times"></i></button>
        <button class="slick-btn next-arrow"><i class="fa fa-chevron-right"></i></button>
        <button class="slick-btn prev-arrow"><i class="fa fa-chevron-left"></i></button>
        <div class="zoom-list">
            <div class="zoom-item" index="0">
                <img src="./img/testImage1.png" alt="image" />
            </div>
            <div class="zoom-item" index="1">
                <img src="./img/testImage2.png" alt="image" />
            </div>
            <div class="zoom-item" index="2">
                <img src="./img/testImage3.png" alt="image" />
            </div>
            <div class="zoom-item" index="3">
                <img src="./img/testImage4.png" alt="image" />
            </div>
            <div class="zoom-item" index="4">
                <img src="./img/testImage5.png" alt="image" />
            </div>
        </div>
    </div>

    <div class="row">
        <!-- ----------------------SIDEBAR---------------------- -->
        <section class="sidebar">
            <div class="logo">
                <img src="./img/blue.png" alt="logo" />
            </div>
            <ul class="menu">
                <li class="menu-item blue">
                    <a href="/">
                        <i class="fa fa-tv"></i>
                        Dashboard
                    </a>
                </li>
                <li class="menu-item orange">
                    <a href="/">
                        <i class="fa fa-user"></i>
                        User
                    </a>
                </li>
                <li class="menu-item blue-light active">
                    <a href="/">
                        <i class="fa fa-database"></i>
                        Data
                    </a>
                </li>
                <li class="menu-item blue">
                    <a href="/">
                        <i class="fas fa-tools"></i>
                        Actions
                    </a>
                </li>
            </ul>
        </section>
        <!-- -----------------------BODY------------------------- -->
        <section class="body">
            <div class="body-header">
                <div class="container">
                    <div class="row between">
                        <form class="search">
                            <i class="fa fa-search"></i>
                            <input type="text" placeholder="Search..." />
                        </form>
                        <div class="user-info">
                            <img class="avatar"
                                 src='https://avataaars.io/?avatarStyle=Circle&topType=Hat&accessoriesType=Prescription02&facialHairType=BeardLight&facialHairColor=BrownDark&clotheType=ShirtScoopNeck&clotheColor=Gray01&eyeType=Default&eyebrowType=Default&mouthType=Default&skinColor=Light' />
                            <span class="name">Công Toàn</span>
                        </div>
                    </div>
                    <div class="breadcumb">
                        <a href="/"><i class="fa fa-home"></i></a>
                        <span>/</span>
                        <p>Data</p>
                        <span>/</span>
                        <p>Images</p>
                    </div>
                </div>
            </div>
            <div class="body-main">
                <div class="container">
                    <div class="table">
                        <h3>24-10-2021</h3>
                        <div class="row data-content">
                            <div class="data-image">
                                <img src="./img/testImage1.png" alt="image" />
                                <p>24-10-2021 14:56:30 PM</p>
                            </div>
                            <div class="data-image">
                                <img src="./img/testImage2.png" alt="image" />
                                <p>24-10-2021 14:56:30 PM</p>
                            </div>
                            <div class="data-image">
                                <img src="./img/testImage3.png" alt="image" />
                                <p>24-10-2021 14:56:30 PM</p>
                            </div>
                            <div class="data-image">
                                <img src="./img/testImage4.png" alt="image" />
                                <p>24-10-2021 14:56:30 PM</p>
                            </div>
                            <div class="data-image">
                                <img src="./img/testImage5.png" alt="image" />
                                <p>24-10-2021 14:56:30 PM</p>
                            </div>

                        </div>
                        <div class="pagination row">
                            <a href="/" class="pagination-item"><i class="fa fa-chevron-left"></i></a>
                            <a href="/" class="pagination-item active">1</a>
                            <a href="/" class="pagination-item">2</a>
                            <a href="/" class="pagination-item">3</a>
                            <a href="/" class="pagination-item"><i class="fa fa-chevron-right"></i></a>
                        </div>
                    </div>
                </div>

            </div>
        </section>
    </div>

    <script src="./js/main.js"></script>
</body>

</html>